<xsd:schema>
	<xsd:element name="p"type="type1"/>
	<xsd:element name="bs"type="type2"/>

	<xsd:complexType name="type1">
		<xsd:sequence>
			<xsd:element name="a"type="xsd:string"/>
			<xsd:element ref="bs"/>
			<xsd:element name="c"type="xsd:string"minOccurs="0"/>
